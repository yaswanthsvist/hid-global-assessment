    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "nodemon": "^1.3.7"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
