{
  "name": "hid-global-assessment",
  "version": "0.1.0",
  "description": "React, Redux, Redux Saga, React-Router,Eslint, Jest, babel, ES6, Flow and Webpack Project",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack-dev-server --open --config webpack.config.dev.js",
    "dev:hot": "npm run dev -- --hot",
    "server": "nodemon ./server/index.js",
    "test": "jest",
    "format": "pretty-quick",
    "lint": "glow"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --single-quote --no-semi --trailing-comma --staged && npm run lint && npm run test"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js"
    }
  },
  "author": "Yaswanth M",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yaswanthsvist/react-boilerplate.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/yaswanthsvist/react-boilerplate/issues"
  },
  "homepage": "https://github.com/yaswanthsvist/react-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "flow-annotation-check": "^1.8.1",
    "flow-bin": "0.57.3",
    "flow-typed": "2.3.0",
    "glow": "^1.2.2",
    "html-webpack-plugin": "^3.2.0",
